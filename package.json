{
  "name": "ngx-uppy-demo",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/server/index.js",
    "dev:server": "nodemon -e ts -w ./server -x npm run server",
    "dev:client": "ng serve --proxy-config proxy.conf.json -o",
    "server": "ts-node --project src/server/tsconfig.json ./src/server/index.ts",
    "deploy": "git push origin master && git push heroku master",
    "build": "npm run build:server && npm run build:client",
    "build:client": "ng build --configuration production",
    "build:server": "tsc --project src/server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server-test": "mocha -r ts-node/register src/server/tests/**/*.spec.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.0.4",
    "@angular/cdk": "^13.3.9",
    "@angular/cli": "15.0.5",
    "@angular/common": "15.0.4",
    "@angular/compiler": "15.0.4",
    "@angular/compiler-cli": "15.0.4",
    "@angular/core": "15.0.4",
    "@angular/forms": "15.0.4",
    "@angular/language-service": "15.0.4",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "15.0.4",
    "@angular/platform-browser-dynamic": "15.0.4",
    "@angular/platform-server": "15.0.4",
    "@angular/router": "15.0.4",
    "@ngx-gallery/core": "^5.0.0-beta.0",
    "@ngx-gallery/gallerize": "^5.0.0-beta.0",
    "@ngx-gallery/lightbox": "^5.0.0-beta.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^14.14.37",
    "@uppy/core": "^1.10.4",
    "@uppy/dashboard": "^1.12.7",
    "@uppy/file-input": "^1.4.17",
    "@uppy/progress-bar": "^1.3.19",
    "@uppy/xhr-upload": "^1.6.4",
    "bootstrap": "^4.5.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "codelyzer": "^6.0.0",
    "core-js": "^3.6.5",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "tslint": "~6.1.3",
    "typedi": "^0.10.0",
    "typescript": "4.8.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.5",
    "@types/chai": "^4.2.16",
    "@types/chai-as-promised": "^7.1.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "mocha": "^8.3.2"
  }
}
