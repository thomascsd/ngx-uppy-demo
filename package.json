{
  "name": "angular-express-starter",
  "version": "0.3.0",
  "license": "MIT",
  "scripts": {
    "start": "ts-node --project src/server/tsconfig.json ./src/server/index.ts",
    "dev:server": "nodemon -e ts -w ./server -x npm run server",
    "dev:client": "ng serve --proxy-config proxy.conf.json",
    "server": "ts-node --project src/server/tsconfig.json ./src/server/index.ts",
    "deploy": "git push origin master && git push heroku master",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ng build --prod"
  },
  "private": true,
  "dependencies": {
